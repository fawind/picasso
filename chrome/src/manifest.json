{
  "manifest_version": 2,
  "name": "Picasso - New Tab Page",
  "description": "Art masterpieces in your browser tabs.",
  "version": "0.0.2",
  "incognito": "split",
  "permissions": [
    "http://picasso-tab.appspot.com/*",
    "https://*.wikiart.org/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "picasso-16.png",
    "48": "picasso-48.png",
    "128": "picasso-128.png"
  }
}
